//Description : Test de l'accès aux attributs après héritage
//
class A {
    int x = 1 ;
    int y = 2 ;
}

class B extends A {
    int z = 3 ;
}

class C extends B {
    int g = 4 ;
}

class D extends C {}


{
    D d = new D();
    println(d.x);
    println(d.y);
    println(d.z);
    println(d.g);
}